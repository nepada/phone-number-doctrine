{
    "name": "nepada/phone-number-doctrine",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Phone number type for Doctrine.",
    "keywords": ["phone number", "value object", "doctrine"],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/phone-number-doctrine/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "doctrine/dbal": "^2.6@dev || ^3.0@dev",
        "brick/phonenumber": "^0.2@dev || ^0.3@dev || ^0.4@dev"
    },
    "require-dev": {
        "nette/tester": "2.4.1",
        "mockery/mockery": "1.4.4",
        "phpstan/phpstan": "0.12.97",
        "phpstan/phpstan-strict-rules": "0.12.11",
        "pepakriz/phpstan-exception-rules": "0.11.7",
        "nepada/phpstan-nette-tester": "0.3.1",
        "spaze/phpstan-disallowed-calls": "1.9.0",
        "phpstan/phpstan-mockery": "0.12.14",
        "php-parallel-lint/php-parallel-lint": "1.3.1",
        "nepada/coding-standard": "7.4.0"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\PhoneNumberDoctrine\\": "src/PhoneNumberDoctrine/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\": "tests/"
        }
    },
    "config": {
        "platform-check": false
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev"
        }
    }
}

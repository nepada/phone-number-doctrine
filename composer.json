{
    "name": "nepada/phone-number-doctrine",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Phone number type for Doctrine.",
    "keywords": ["phone number", "value object", "doctrine"],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/phone-number-doctrine/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=7.4.0 <8.3",
        "doctrine/dbal": "^2.13.3@dev || ^3.0@dev",
        "brick/phonenumber": "^0.2@dev || ^0.3@dev || ^0.4@dev || ^0.5@dev"
    },
    "require-dev": {
        "nette/tester": "2.4.3",
        "mockery/mockery": "1.5.1",
        "phpstan/phpstan": "1.10.3",
        "phpstan/phpstan-strict-rules": "1.4.5",
        "nepada/phpstan-nette-tester": "1.0.0",
        "spaze/phpstan-disallowed-calls": "2.12.1",
        "shipmonk/phpstan-rules": "2.3.1",
        "phpstan/phpstan-mockery": "1.1.1",
        "php-parallel-lint/php-parallel-lint": "1.3.2",
        "nepada/coding-standard": "7.7.0",
        "giggsey/libphonenumber-for-php": ">=8.12.34"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\PhoneNumberDoctrine\\": "src/PhoneNumberDoctrine/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\": "tests/"
        }
    },
    "config": {
        "platform-check": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev"
        }
    }
}

{
    "name": "nepada/phone-number-doctrine",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Phone number type for Doctrine.",
    "keywords": ["phone number", "value object", "doctrine"],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/phone-number-doctrine/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=8.1.0 <8.5",
        "doctrine/dbal": "^3.4@dev || ^4.0@dev",
        "brick/phonenumber": "^0.2.2@dev || ^0.3@dev || ^0.4@dev || ^0.5@dev || ^0.6@dev || ^0.7@dev || ^0.8@dev"
    },
    "require-dev": {
        "nette/tester": "2.5.5",
        "mockery/mockery": "1.6.12",
        "phpstan/phpstan": "2.1.17",
        "phpstan/phpstan-strict-rules": "2.0.4",
        "nepada/phpstan-nette-tester": "2.0.0",
        "spaze/phpstan-disallowed-calls": "4.5.0",
        "shipmonk/phpstan-rules": "4.1.4",
        "phpstan/phpstan-mockery": "2.0.0",
        "php-parallel-lint/php-parallel-lint": "1.4.0",
        "nepada/coding-standard": "7.15.1",
        "composer-runtime-api": "^2.0",
        "composer/semver": "3.4.3",
        "giggsey/libphonenumber-for-php": ">=8.13.47"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\PhoneNumberDoctrine\\": "src/PhoneNumberDoctrine/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\": "tests/"
        }
    },
    "config": {
        "platform-check": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.3-dev"
        }
    }
}
